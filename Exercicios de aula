#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
float num1= 6.25;
printf("voces verão tudo com erro de acento \n");
printf("numero 1: %.2f\n",num1);

setlocale(LC_ALL,"Portuguese");
float num2=6.25;

printf("vocês não verão mais erros de acento \n");
printf("numero 2: %.2f\n",num2);


return(0);
}
