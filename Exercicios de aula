#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#define pi 3.14;

int main()
{
float raio, area;

printf("Digite o raio \n");
scanf("%f", &raio);
area = pow(raio,2)*pi;
printf("Area total do circulo: %.2f\n", area);
return 0;
}
